{
  "description": "vim basic enter and shift enter",
  "manipulators": [
    {
      "from": {
        "key_code": "q",
        "modifiers": {
          "mandatory": ["fn"],
          "optional": ["any"]
        }
      },
      "to": [
        {
          "key_code": "tab"
        }
      ],
      "type": "basic"
    },
    {
      "from": {
        "key_code": "w",
        "modifiers": {
          "mandatory": ["fn"],
          "optional": ["any"]
        }
      },
      "to": [
        {
          "key_code": "tab",
          "modifiers": ["left_shift"]
        }
      ],
      "type": "basic"
    }
  ]
}
