{
  "description": "Map fn + space to CMD + space",
  "manipulators": [
    {
      "type": "basic",
      "from": {
        "key_code": "spacebar",
        "modifiers": {
          "mandatory": ["fn"],
          "optional": ["any"]
        }
      },
      "to": [
        {
          "key_code": "spacebar",
          "modifiers": ["left_command"]
        }
      ]
    }
  ]
}
